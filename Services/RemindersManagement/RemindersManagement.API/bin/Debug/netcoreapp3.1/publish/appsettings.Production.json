{
  "Data": {
    "ConnectionString": "Host=localhost;Database=FriendReminders;Username=postgres;Password=password"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "RemindersManagementService",
        "Uri": "http://localhost/health"
      }
    ],
    "EvaluationTimeOnSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "Serilog": {
    "Using": [
      "AWS.Logger.SeriLog",
      "Serilog.Sinks.Console"
    ],
    "LogGroup": "FriendReminders.RemindersManagementService",
    "Region": "ap-southeast-2",
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "AWSSeriLog"
      },
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*"
}
